class(NA)
class(NaN)
class("")
mode(Inf)
mode(NA)
mode(NaN)
mode("")
storage.mode(Inf)
storage.mode(NaN)
storage.mode(NA)
storage.mode("")

#问题二
pat_name <- c("dog" , "cat" , "dolphin", "hamster" , "goldfish")
result <- (sample(pat_name , 100 , replace = TRUE))
head(result)
table(result)

